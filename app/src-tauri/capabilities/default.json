{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "deep-link:default",
    "store:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [{"url": "http://localhost:3000"}]
    }
  ]
}